

package com.practicemodepaul;

import java.util.Scanner;

/**
 * @author johnpaul
 * if-else in Java.
 */
public class Practicemodepaul {

    public static void main(String args[]) {

        
        
  
int x = 4;
int y = 18;
if
 (x == y) {
  System.out.println("You may proceed to 2nd verification for your age");
} 
else if
 (x > y) {
  System.out.println("You may now proceed to the next application");

} else {
  System.out.println("Your are not allowed,only 18 yrs old above");
} 
      
}

}
